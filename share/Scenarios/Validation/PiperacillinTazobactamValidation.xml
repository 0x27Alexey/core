	<DataRequest xsi:type="SubstanceDataRequestData" Substance="Piperacillin" Name="MassInBody" Unit="ug" Precision="4"/>	
	<DataRequest xsi:type="SubstanceDataRequestData" Substance="Piperacillin" Compartment="LeftKidneyTissue" Name="PartitionCoefficient" Precision="4"/>
